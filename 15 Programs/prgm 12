public class Main {  
    public static void main(String[] args) {  
      
        int [] arr = new int [] {10, 12, 14, 15, 16};  
        System.out.println("Elements of given array: ");  
    
        for (int i = 0; i < arr.length; i++) {  
            System.out.print(arr[i] + " ");  
        }  
    }  
}  
